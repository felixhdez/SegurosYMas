    <script src="~/Scripts/wow.min.js"></script>

    <script>
        

        $(document).ready(function () {

            $('body').scrollspy({
                target: '.navbar-fixed-top',
                offset: 80
            });

            // Page scrolling feature
            $('a.page-scroll').bind('click', function (event) {
                var link = $(this);
                $('html, body').stop().animate({
                    scrollTop: $(link.attr('href')).offset().top - 50
                }, 500);
                event.preventDefault();
                $('#navbar').collapse('hide');
            });
        });

        var cbpAnimatedHeader = (function () {
            var docElem = document.documentElement,
                header = document.querySelector('.navbar-default'),
                didScroll = false,
                changeHeaderOn = 200;
            function init() {
                window.addEventListener('scroll', function (event) {
                    if (!didScroll) {
                        didScroll = true;
                        setTimeout(scrollPage, 250);
                    }
                }, false);
            }
            function scrollPage() {
                var sy = scrollY();
                if (sy >= changeHeaderOn) {
                    $(header).addClass('navbar-scroll')
                    $("#img1").attr("src", "/Content/img/SyM colores.png");//cambio de imagen con scroll
                    $('.navbar-brand').css('background', 'none');//quitar el color del fondo
                    $('.btn-inicio').css('margin-top', '22px')//cambiar la posicion de inicio de sesion al hacer scroll

                }
                else {
                    $(header).removeClass('navbar-scroll')
                    $("#img1").attr("src", "/Content/img/SyM blanco.png");//cambio de imagen con scroll
                    $('.navbar-brand').css('background', 'none');//cambiar el color del fondo
                    $('.btn-inicio').css('margin-top', '26px')//cambiar la posicion de inicio de sesion al hacer scroll
                }
                didScroll = false;
            }
            function scrollY() {
                return window.pageYOffset || docElem.scrollTop;
            }
            init();

        })();

        //Activar wow.js plugin para la animacion en scroll
        new WOW().init();

    </script>
</body>
